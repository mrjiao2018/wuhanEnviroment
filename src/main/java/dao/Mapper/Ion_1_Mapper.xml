<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.Mapper.Ion_1_Mapper">
    <!--根据id查询数据-->
    <select id="select" resultType="vo.lon.Ion_1_calculator">
        select * from ion_1_calculator where year=#{year}
    </select>

    <insert id="insert" parameterType="vo.lon.Ion_1_calculator">
        insert into ion_1_calculator values (#{a},#{h},#{q},#{l},#{year},#{position},#{result});
    </insert>

</mapper>