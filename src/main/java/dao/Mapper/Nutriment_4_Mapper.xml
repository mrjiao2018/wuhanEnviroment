<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.Mapper.Nutriment_4_Mapper">
    <!--根据id查询数据-->
    <select id="select" resultType="vo.nutriment.Nutriment_4_calculator">
        select * from nutriment_4_calculator where year=#{year}
    </select>

    <insert id="insert" parameterType="vo.nutriment.Nutriment_4_calculator">
        insert into nutriment_4_calculator values (#{a},#{b},#{c1},#{c2},#{r1},#{r2},#{r3},#{p},#{k},#{n},#{year},#{position},#{result});
    </insert>

</mapper>