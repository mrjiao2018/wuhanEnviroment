<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.Mapper.Soil_6_Mapper">
    <!--根据id查询数据-->
    <select id="select" resultType="vo.soil.Soil_6_calculator">
        select * from soil_6_calculator where year=#{year}
    </select>

    <insert id="insert" parameterType="vo.soil.Soil_6_calculator">
        insert into soil_6_calculator values (#{a},#{x1},#{x2},#{c1},#{c2},#{c3},#{r1},#{r2},#{r3},#{m},#{p},#{k},#{n},#{year},#{position},#{result})
    </insert>

</mapper>