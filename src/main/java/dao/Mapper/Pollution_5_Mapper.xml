<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.Mapper.Pollution_5_Mapper">
    <!--根据id查询数据-->
    <select id="select" resultType="vo.pollution.Pollution_5_calculator">
        select * from pollution_5_calculator where year=#{year}
    </select>

    <insert id="insert" parameterType="vo.pollution.Pollution_5_calculator">
        insert into pollution_5_calculator values (#{a},#{q},#{year},#{position},#{result})
    </insert>

</mapper>